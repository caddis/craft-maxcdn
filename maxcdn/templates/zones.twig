{% extends 'maxcdn/layouts/cp' %}

{% set selectedTab = 'zones' %}

{% set content %}
	<table class="data fullwidth">
		<thead>
			<tr>
				<th scope="col">{{ 'Zone'|t }}</th>
				<th scope="col">{{ 'Cache Hits'|t }}</th>
				<th scope="col">{{ 'Hits'|t }}</th>
				<th scope="col">{{ 'Non-Cache Hits'|t }}</th>
				<th scope="col">{{ 'Size'|t }}</th>
			</tr>
		</thead>
		<tbody>
			{% for zone in zones %}
				<tr>
					{% for stat in zone %}
						<td>{{ stat }}</td>
					{% endfor %}
				</tr>
			{% endfor %}
		</tbody>
	</table>
{% endset %}